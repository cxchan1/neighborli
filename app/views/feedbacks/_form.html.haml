= form_with(model: feedback, local: true) do |form|
  - if feedback.errors.any?
    #error_explanation
      %h2= "#{pluralize(feedback.errors.count, "error")} prohibited this feedback from being saved:"
      %ul
       - feedback.errors.full_messages.each do |message|
        %li= message
  .field
    = form.label :user_id
    = form.number_field :user_id
  .field
    = form.label :rating
    = form.number_field :rating
  .field
    = form.label :transaction_id
    = form.number_field :transaction_id
  .field
    = form.label :comment
    = form.text_field :comment
  .actions
    = form.submit